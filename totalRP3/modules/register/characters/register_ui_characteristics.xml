	<!-- Common icon button  -->
	<Button name="TRP3_MapHereButton" virtual="true">
		<Size x="16" y="16" />
		<NormalTexture file="Interface\ICONS\inv_misc_rune_01" />
		<!-- <HighlightTexture alphaMode="ADD" file="Interface\Buttons\UI-Quickslot-Depress"/> -->
